<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jeromegout.learningtest">

    <application
        android:name=".Initializer"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.EditClassActivity">
        </activity>
        <activity android:name=".activities.EditStudentActivity">
        </activity>
        <activity android:name=".activities.ClassActivity">
        </activity>
        <activity android:name=".activities.BackActivity">
        </activity>
        <activity android:name=".activities.RandomDrawActivity">
        </activity>
        <activity android:name=".activities.StudentActivity">
        </activity>
        <activity android:name=".activities.StudentGradeActivity"/>
        <activity android:name=".activities.GradeActivity" />
    </application>

</manifest>